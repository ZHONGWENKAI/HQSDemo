<?xml version="1.0" encoding="utf-8"?>
<Star>
  <!--调试开关。默认false-->
  <Debug>false</Debug>
  <!--服务端地址。如http://star.newlifex.com:6600，默认为空-->
  <Server>http://192.168.1.120:6600</Server>
  <!--应用标识-->
  <AppKey></AppKey>
  <!--应用密钥-->
  <Secret></Secret>
  <!--服务地址。人工设定，用于提交注册中心，默认为空，自动识别外部访问地址-->
  <ServiceAddress></ServiceAddress>
  <!--跟踪采样周期。默认60s-->
  <TracerPeriod>60</TracerPeriod>
  <!--最大正常采样数。采样周期内，最多只记录指定数量的正常事件，用于绘制依赖关系，默认1-->
  <MaxSamples>1</MaxSamples>
  <!--最大异常采样数。采样周期内，最多只记录指定数量的异常事件，默认10-->
  <MaxErrors>10</MaxErrors>
</Star>